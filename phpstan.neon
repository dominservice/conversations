parameters:
  level: 5
  paths:
    - src
  excludePaths:
    - vendor
  checkMissingIterableValueType: false
  ignoreErrors:
    - '#Call to an undefined method [a-zA-Z0-9\\_]+::newQuery\(\)#'
    - '#Call to an undefined method [a-zA-Z0-9\\_]+::create\(\)#'
    - '#Call to an undefined method [a-zA-Z0-9\\_]+::with\(\)#'
    - '#Call to an undefined method [a-zA-Z0-9\\_]+::find\(\)#'
    - '#Call to an undefined method [a-zA-Z0-9\\_]+::where\(\)#'