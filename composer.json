    "homepage": "https://github.com/dominservice/conversations",
    "license": "MIT",
    "authors": [
        {
            "name": "dominservice",
            "email": "biuro@dso.biz.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "^5.6|^5.7|^5.8|^6|^7|^8"
    },
    "require-all": true,
    "autoload": {
        "psr-4": {
